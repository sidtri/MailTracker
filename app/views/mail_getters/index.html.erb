<p id="notice"><%= notice %></p>

<h1>Mail Getters</h1>

<table>
  <thead>
    <tr>
      <th>Method</th>
      <th>Address</th>
      <th>Port</th>
      <th>User name</th>
      <th>Password</th>
      <th>Enable ssl</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mail_getters.each do |mail_getter| %>
      <tr>
        <td><%= mail_getter.method %></td>
        <td><%= mail_getter.address %></td>
        <td><%= mail_getter.port %></td>
        <td><%= mail_getter.user_name %></td>
        <td><%= mail_getter.password %></td>
        <td><%= mail_getter.enable_ssl %></td>
        <td><%= mail_getter.user %></td>
        <td><%= link_to 'Show', mail_getter %></td>
        <td><%= link_to 'Edit', edit_mail_getter_path(mail_getter) %></td>
        <td><%= link_to 'Destroy', mail_getter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mail Getter', new_mail_getter_path %>
